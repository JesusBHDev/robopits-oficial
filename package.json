    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!axios).+\\.js$"
    ],
    "moduleNameMapper": {
      "^axios$": "<rootDir>/src/__mocks__/axios.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/**/index.js"
    ]
  }
